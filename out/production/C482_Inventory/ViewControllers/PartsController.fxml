<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewControllers.PartsController">
   <children>
      <VBox prefHeight="600.0" prefWidth="803.0">
         <children>
            <HBox prefHeight="55.0" prefWidth="789.0">
               <children>
                  <Label fx:id="label" text="Add Part">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="inHouse" mnemonicParsing="false" text="In-House">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="outsourced" mnemonicParsing="false" text="Outsourced">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </HBox.margin>
                  </RadioButton>
               </children>
            </HBox>
            <HBox prefHeight="483.0" prefWidth="803.0">
               <children>
                  <VBox prefHeight="356.0" prefWidth="178.0">
                     <children>
                        <Label text="ID">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                        <Label text="Name">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                        <Label text="Inventory">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                        <Label text="Price/Cost">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                        <Label text="Max">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="secondaryLabel" text="Company Name">
                           <VBox.margin>
                              <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox prefHeight="305.0" prefWidth="395.0">
                     <children>
                        <TextField fx:id="id">
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="name">
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="stock">
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="price">
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                        <HBox prefHeight="33.0" prefWidth="395.0">
                           <children>
                              <TextField fx:id="max" />
                              <Label text="Min">
                                 <HBox.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="min" />
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <TextField fx:id="secondary">
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="55.0" prefWidth="781.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#savePartAction" text="Save">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#cancelAction" text="Cancel">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
